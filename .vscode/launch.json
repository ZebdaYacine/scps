{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "app",
            "cwd": "frontend\\app",
            "request": "launch",
            "type": "dart",
            "args": ["--web-renderer", "html"]
        },
        {
            "name": "app (profile mode)",
            "cwd": "frontend\\app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": ["--web-renderer", "html"]
        },
        {
            "name": "app (release mode)",
            "cwd": "frontend\\app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": ["--web-renderer", "html"]
        }
    ]
}



